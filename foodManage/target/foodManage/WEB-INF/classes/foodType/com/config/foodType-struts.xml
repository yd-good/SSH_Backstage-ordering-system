<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <package name="foodTypeAction" extends="struts-default" strict-method-invocation="false">
        <action name="foodType_*" class="foodType.com.action.FoodTypeAction" method="{1}">
            <result name="list" >/sys/cuisineList.jsp</result>
            <result name="updateCuisine">/sys/updateCuisine.jsp</result>
            <result name="dishesList">/sys/foodList.jsp</result>
            <result name="showUpFood">/sys/updateFood.jsp</result>
        </action>
    </package>
</struts>